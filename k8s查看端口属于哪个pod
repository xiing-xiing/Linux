netstat -nlpt |grep -w {端口名称} |awk '{print$NF}'|awk -F'\\/' '{print$1}' |while read linn;do ps -efww | grep -v grep | grep  $linn  | while read line; do   echo "$line";   pid=$(echo "$line" | awk '{print $2}');   while test ${pid:-1} -ne 1;   do     data="$(ps --no-headers -efww -q $pid)";     echo "$data";     pid=$(echo "$data" | awk '{print $3}');   done; done ; done |grep containerd-shim |grep -oP  '(?<=moby/).+(?=-address)'  |xargs docker inspect -f '{{.Name}}'
